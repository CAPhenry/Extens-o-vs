{
  "name": "henry-snippets",
  "displayName": "Henry-snippets",
  "description": "Apenas uma extens√£o para ajudar no desenvolvimento de scripts!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "icon": "logo.jpg",
  "categories": [
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
        {
            "language": "lua",
            "path": "./snippets/lua.json"
        },
        {
            "language": "javascript",
            "path": "./snippets/javascript.json"
        },
        {
            "language": "html",
            "path": "./snippets/html.json"
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
